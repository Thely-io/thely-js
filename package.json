{
  "name": "thely",
  "version": "1.5.3",
  "description": "Thely.io JavaScript SDK, allows you to integrate Thely.io into your website or app.",
  "module": "out/index.js",
  "main": "out/index.js",
  "author": "ajxb",
  "private": false,
  "files": [
    "lib",
    "index.*",
    "types.*",
    "utils.*",
    "schemas.*",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "prepare": "cd src && tsc --emitDeclarationOnly && cd .. && git add . && git diff --cached --exit-code || (git commit -m 'adding types' && git push)",
    "build": "cd src && bun build ./index.ts ./schemas.ts ./types.ts ./utils.ts ./lib/hooks/*  --outdir ../ --target browser",
    "release": "npm version patch && npm publish"
  },
  "exports": {
    "types": "./types.d.ts",
    "utils": "./utils.d.ts",
    "schemas": "./schemas.d.ts",
    "lib/hooks/useQuery": "./lib/hooks/useQuery.d.ts",
    "lib/hooks/useInit": "./lib/hooks/useInit.d.ts",
    "lib/hooks/useMe": "./lib/hooks/useMe.d.ts",
    "lib/hooks/useShippingRates": "./lib/hooks/useShippingRates.d.ts"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.1",
    "prettier": "^3.3.2",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@ajxb/userequest": "^1.3.2",
    "axios": "^1.6.8",
    "countries-list": "^3.1.0",
    "react": "^18.3.1",
    "swr": "^2.2.5",
    "zod": "^3.23.5"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "useTabs": true
  }
}

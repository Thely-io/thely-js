{
  "name": "thely",
  "version": "1.0.13",
  "description": "Thely.io JavaScript SDK, allows you to integrate Thely.io into your website or app.",
  "module": "out/index.js",
  "main": "out/index.js",
  "author": "ajxb",
  "private": false,
  "type": "module",
  "scripts": {
    "prepare": "cd src && tsc --emitDeclarationOnly && git add . && git commit -m 'chore: update types' && git push",
    "build": "cd src && bun build ./index.ts ./schemas.ts ./types.ts ./utils.ts ./lib/hooks/*  --outdir ../ --target browser",
    "release": "npm version patch && npm publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.1",
    "prettier": "^3.3.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ajxb/userequest": "^1.3.2",
    "axios": "^1.6.8",
    "countries-list": "^3.1.0",
    "react": "^18.3.1",
    "swr": "^2.2.5",
    "zod": "^3.23.5"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "useTabs": true
  }
}
